package com.rituj.threading;

class MyThread9 extends Thread{
	public void run() {
		for (int i = 0; i < 3; i++) {
			System.out.println("THread 1 ");
			try {
				Thread.sleep(2000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
		}
	}
}
public class ThreadJoinDemo {

}
